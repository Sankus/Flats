
@{
    ViewBag.Title = "Premium";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2 class="prew_head_name">Premium</h2>

@foreach (var obj in ViewBag.list) 
{ 
    <div id="obj_prew obj_@obj.ID">
        <div class="img_prew">
        <div><img id="large_foto1_pic"  src="/Home/GetPictureFL1?id=@obj.ID"></div>
        </div>
        <div class="left_cont_prew">
            <div class="name_obj_prew"><h2>@obj.header</h2></div>
            <div class="street_prew">@obj.address</div>
       <div class="price_prew">
           <div class="obj_price_in price_prew_1"><div class="before_price_prew">1 Сутки</div><div class="inner_price_prew">@obj.price1 грн</div></div>
           <div class="obj_price_in price_prew_2"><div class="before_price_prew">2-4 Суток</div><div class="inner_price_prew">@obj.price2 грн</div></div>
           <div class="obj_price_in price_prew_3"><div class="before_price_prew">5+ Суток</div><div class="inner_price_prew">@obj.price5 грн</div></div>
        <div class="obj_price_in price_prew_4"><div class="before_price_prew">14+ Суток</div><div class="inner_price_prew">@obj.price14 грн</div></div>
       </div>
            </div>
        <div class="right_cont_prew">
            <div class="star_prew">Тут выводим звезды</div>
            <div class="in_flat_prew">Тут выводим данные квартиры</div>
            <div class="bottom_prew">
                <div class="bottom_prew_1">
                    <a class="bottom_ac_prew" href="#">Забронировать</a>
                    </div>
                <div class="bottom_prew_1">
                    <a class="bottom_ac_prew _ac_prew2" href="#">Подробнее</a>
                    </div>
                </div>
        </div>
        <div id="faq">
            <h2>Вопрос №1</h2>
            <div>
                <p>Ответ на вопрос №1</p>
            </div>
            <h2>Вопрос №2</h2>
            <div>
                <p>Ответ на вопрос №2</p>
            </div>
            <h2>Вопрос №3</h2>
            <div>
                <p>Ответ на вопрос №3</p>
            </div>
        </div>
            <div>@obj.desc_body</div>




        </div>
}